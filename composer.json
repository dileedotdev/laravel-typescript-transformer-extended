{
    "name": "dinhdjj/laravel-typescript-transformer-extended",
    "description": "A set of transformers for spatie/laravel-typescript-transformer",
    "keywords": [
        "dinhdjj",
        "laravel",
        "typescript-transformer-extended"
    ],
    "homepage": "https://github.com/dinhdjj/typescript-transformer-extended",
    "license": "MIT",
    "authors": [
        {
            "name": "dinhdjj",
            "email": "dinhdjj@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Dinhdjj\\TypescriptTransformerExtended\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dinhdjj\\TypescriptTransformerExtended\\Database\\Factories\\": "database/factories",
            "Dinhdjj\\TypescriptTransformerExtended\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
